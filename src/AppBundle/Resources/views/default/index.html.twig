<html>
<head>
    <title>Symfony2 - AngularJS - RabbitMQ Websocket demo</title>

    <script src="//cdnjs.cloudflare.com/ajax/libs/sockjs-client/0.3.4/sockjs.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/stomp.js/2.3.3/stomp.min.js"></script>

    <!-- AngularJS 1.4 base -->
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.9/angular.js"></script>
    <!-- UI-Bootstrap directives -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-bootstrap/1.3.3/ui-bootstrap-tpls.min.js"></script>
    <!-- UI Router directive -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.3.1/angular-ui-router.min.js"></script>
    <!-- Boootstrap CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">

        {% javascripts
            '@AppBundle/Resources/views/default/js/app.js'
            '@AppBundle/Resources/views/default/js/ApproveOrderController.js'
            '@AppBundle/Resources/views/default/js/SubmitOrderController.js'
                output='js/realtimeapp-angular.js'%}
            <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}

</head>
<!-- 3. Display the application -->
<body ng-app="realtimeApp">
    <nav class="navbar navbar-inverse navbar-static-top">
        <a class="navbar-brand" href="#">Symfony2 - AngularJS - RabbitMQ Websocket demo</a>
    </nav>
    <div class="container">
        <div class="panel panel-primary">
            <div class="panel-body">
                <div ui-view></div>
            </div> <!-- End of panel body -->
        </div>
    </div>

    <!-- include the partials -->
    {{include ('AppBundle:default:partials/home.html.twig')}}
    {{include ('AppBundle:default:partials/submitOrder.html.twig')}}
    {{include ('AppBundle:default:partials/approveOrder.html.twig')}}
</body>
</html>